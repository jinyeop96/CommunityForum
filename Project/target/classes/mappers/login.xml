<mapper namespace="com.project.dao.LoginDAO">
	<select id="selectUser" resultType="com.project.dto.JoinDTO">
		SELECT * FROM hmember WHERE j_id = #{j_id} and j_pwd=#{j_pwd}
	</select>
</mapper>